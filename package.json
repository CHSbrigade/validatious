{
  "name": "@chsbrigade/schema-fu",
  "version": "0.1.0",
  "description": "\"Utility to validate plain javascript objects based on declared schemas.\"",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest --watch"
  },
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --single-quote --write",
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "keywords": [
    "schema",
    "json",
    "validation"
  ],
  "author": "chsbrigade",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.1",
    "prettier": "^1.5.2"
  },
  "dependencies": {
    "ramda": "^0.24.1"
  }
}
